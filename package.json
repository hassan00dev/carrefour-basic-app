    "build": "NODE_OPTIONS=--max_old_space_size=4096 webpack --node-env=production --env environment=production",
    "build:stats": "NODE_OPTIONS=--max_old_space_size=4096 webpack --node-env=production --env environment=production --json > stats.json",
    "clean": "rm -rf node_modules && rm yarn.lock",
    "clean:start": "yarn clean && yarn && yarn start",
    "build:deploy": "yarn clean && yarn && yarn build",
    "build:deploy:dev": "yarn clean && yarn && yarn build:dev",
    "win-build:deploy": "yarn win-clean && yarn && yarn win-build",
    "win-clean": "del /F /Q node_modules",
    "win-build": "set NODE_OPTIONS=--max_old_space_size=4096 && webpack --node-env=production --env environment=production",
